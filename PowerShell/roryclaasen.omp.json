{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "block": "darkGray",
    "block-dir": "green",
    "error": "red",
    "foreground": "#88c0d0",
    "foreground-misc": "#d8dee9",
    "root": "lightYellow"
  },
  "debug_prompt": {
    "foreground": "p:foreground"
  },
  "secondary_prompt": {
    "template": "> ",
    "foreground": "p:foreground"
  },
  "console_title_template": "{{if .Root}}⚡ {{end}}{{.Folder}} - {{.Shell}} {{.ShellVersion}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "<p:block>┏</>",
          "type": "text"
        },
        {
          "style": "plain",
          "template": "<p:block>[</>{{ .UserName }}<p:block>]</>",
          "foreground": "p:foreground",
          "type": "session"
        },
        {
          "style": "plain",
          "foreground": "p:foreground",
          "type": "git",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "source": "pwsh",
            "branch_icon": " ",
            "branch_identical_icon": "≡",
            "branch_behind_icon": "⇣",
            "branch_ahead_icon": "⇡"
          },
          "templates": [
            "<p:block>-[</>",
            "{{ url .UpstreamIcon .UpstreamURL }} ",
            "{{ .HEAD }}",
            "{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}",
            "{{ if .Staging.Changed }} <green>{{ .Staging.String }}</>{{ end }}",
            "{{ if and (.Working.Changed) (.Staging.Changed) }} <p:block>|</p>{{ end }}",
            "{{ if .Working.Changed }} <red>{{ .Working.String }}</>{{ end }}",
            "<p:block>]</>"
          ]
        },
        {
          "style": "plain",
          "template": "<p:block>-[</><p:block>]</>",
          "foreground": "yellow",
          "type": "root"
        },
        {
          "style": "plain",
          "template": "<p:block>-[</>x{{ .Meaning }}<p:block>]</>",
          "foreground": "p:foreground",
          "type": "status"
        },
        {
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "style": "plain",
          "template": "<p:block>-[</> {{ .FormattedMs }}<p:block>]</>",
          "foreground": "p:foreground",
          "type": "executiontime"
        },
        {
          "style": "plain",
          "template": "<p:block>-[</> {{ .Full }}<p:block>]</>",
          "foreground": "#b915ff",
          "type": "dotnet"
        },
        {
          "style": "plain",
          "template": "<p:block>-[</> {{ .Full }}<p:block>]</>",
          "foreground": "#f6901d",
          "type": "java"
        },
        {
          "style": "plain",
          "template": "<p:block>-[</>󰎙 {{ .Full }}<p:block>]</>",
          "foreground": "#709d53",
          "type": "node"
        },
        {
          "style": "plain",
          "template": "<p:block>-[</>  {{ .Context  }}<p:block>]</>",
          "foreground": "#1c90ed",
          "type": "docker"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "<p:block>┖</>",
          "type": "text"
        },
        {
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "<p:block>[</>{{ .Path }}<p:block>]</>",
          "foreground": "p:foreground",
          "type": "path"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "$",
          "foreground": "p:foreground",
          "type": "status",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:error{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
